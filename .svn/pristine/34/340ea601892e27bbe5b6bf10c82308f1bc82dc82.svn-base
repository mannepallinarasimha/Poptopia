package com.kelloggs.promotions.lib.model;

import java.util.List;

public class PromoCodeMetaObject {

	 private Integer profileId;
	 private Integer promotionId;
	 private String regex ;
	 private List<String> batchCodes;
	/**
	 * @return the profileId
	 */
	public Integer getProfileId() {
		return profileId;
	}
	/**
	 * @param profileId the profileId to set
	 */
	public void setProfileId(Integer profileId) {
		this.profileId = profileId;
	}
	/**
	 * @return the promotionId
	 */
	public Integer getPromotionId() {
		return promotionId;
	}
	/**
	 * @param promotionId the promotionId to set
	 */
	public void setPromotionId(Integer promotionId) {
		this.promotionId = promotionId;
	}
	/**
	 * @return the regex
	 */
	public String getRegex() {
		return regex;
	}
	/**
	 * @param regex the regex to set
	 */
	public void setRegex(String regex) {
		this.regex = regex;
	}
	/**
	 * @return the batchCodes
	 */
	public List<String> getBatchCodes() {
		return batchCodes;
	}
	/**
	 * @param batchCodes the batchCodes to set
	 */
	public void setBatchCodes(List<String> batchCodes) {
		this.batchCodes = batchCodes;
	}
	 
	 
}
