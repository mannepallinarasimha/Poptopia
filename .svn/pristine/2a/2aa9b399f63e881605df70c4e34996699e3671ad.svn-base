package com.kelloggs.promotions.lib.model;

import java.util.Objects;

public class RedeemRewardMetaData {

	private Integer maxRewardPerUser;
	private Integer exchangeCodePerReward;
	/**
	 * @return the maxRewardPerUser
	 */
	public Integer getMaxRewardPerUser() {
		return Objects.nonNull(maxRewardPerUser) ? maxRewardPerUser : 1;
	}
	/**
	 * @param maxRewardPerUser the maxRewardPerUser to set
	 */
	public void setMaxRewardPerUser(Integer maxRewardPerUser) {
		this.maxRewardPerUser = maxRewardPerUser;
	}
	/**
	 * @return the exchangeCodePerReward
	 */
	public Integer getExchangeCodePerReward() {
		return exchangeCodePerReward;
	}
	/**
	 * @param exchangeCodePerReward the exchangeCodePerReward to set
	 */
	public void setExchangeCodePerReward(Integer exchangeCodePerReward) {
		this.exchangeCodePerReward = exchangeCodePerReward;
	}
	
	
}
